{
    "name": "@theunwalked/dreadcabinet",
    "version": "0.0.3",
    "description": "dreadcabinet organizes the chaos.",
    "main": "dist/dreadcabinet.cjs",
    "module": "dist/dreadcabinet.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SemicolonAmbulance/dreadcabinet.git"
    },
    "packageManager": "pnpm@10.12.4",
    "exports": {
        ".": {
            "types": "./dist/dreadcabinet.d.ts",
            "import": "./dist/dreadcabinet.js",
            "require": "./dist/dreadcabinet.cjs"
        }
    },
    "scripts": {
        "build": "pnpm run lint && tsc --noEmit && vite build",
        "start": "dist/main.js",
        "dev": "vite build --watch",
        "watch": "pnpm run dev",
        "test": "pnpm run test:readme && pnpm run test:coverage",
        "test:coverage": "vitest run --coverage",
        "test:readme": "doccident -c .markdown-doctest-setup.mjs README.md",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run build"
    },
    "keywords": [
        "focus",
        "productivity",
        "notes",
        "genai"
    ],
    "author": "Semicolon Ambulance <semicolonambulance@proton.me>",
    "license": "Apache-2.0",
    "dependencies": {
        "commander": "^14.0.0",
        "dayjs": "^1.11.13",
        "dotenv": "^17.0.1",
        "glob": "^11.0.3",
        "js-yaml": "^4.1.0",
        "luxon": "^3.6.1",
        "moment-timezone": "^0.6.0",
        "winston": "^3.17.0",
        "zod": "^3.25.71"
    },
    "devDependencies": {
        "@babel/core": "^7.27.3",
        "@babel/plugin-transform-modules-commonjs": "^7.27.1",
        "@babel/plugin-transform-typescript": "^7.27.1",
        "@babel/preset-typescript": "^7.27.1",
        "@doccident/doccident": "^0.0.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.30.1",
        "@rollup/plugin-replace": "^6.0.2",
        "@swc/core": "^1.12.9",
        "@theunwalked/cardigantime": "^0.0.12",
        "@types/js-yaml": "^4.0.9",
        "@types/luxon": "^3.6.2",
        "@types/node": "^24.0.10",
        "@typescript-eslint/eslint-plugin": "^8.35.1",
        "@typescript-eslint/parser": "^8.35.1",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.30.1",
        "eslint-plugin-import": "^2.32.0",
        "globals": "^16.3.0",
        "mockdate": "^3.0.5",
        "typescript": "^5.8.3",
        "vite": "^7.0.1",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-node": "^7.0.0",
        "vitest": "^3.2.4"
    },
    "pnpm": {
        "overrides": {
            "wrap-ansi": "7.0.0"
        }
    }
}