{
    "name": "@theunwalked/dreadcabinet",
    "version": "0.0.1",
    "description": "dreadcabinet organizes the chaos.",
    "main": "dist/dreadcabinet.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SemicolonAmbulance/dreadcabinet.git"
    },
    "packageManager": "pnpm@10.11.0",
    "exports": {
        ".": {
            "import": "./dist/dreadcabinet.js",
            "types": "./dist/dreadcabinet.d.ts"
        }
    },
    "scripts": {
        "build": "tsc --noEmit && vite build",
        "start": "dist/main.js",
        "dev": "vite",
        "watch": "vite build --watch",
        "test": "pnpm run test:coverage && pnpm run test:readme",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
        "test:readme": "doccident -c .markdown-doctest-setup.mjs README.md",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run build"
    },
    "keywords": [
        "focus",
        "productivity",
        "notes",
        "genai"
    ],
    "author": "Semicolon Ambulance <semicolonambulance@proton.me>",
    "license": "Apache-2.0",
    "dependencies": {
        "commander": "^14.0.0",
        "dayjs": "^1.11.13",
        "dotenv": "^16.5.0",
        "glob": "^11.0.2",
        "js-yaml": "^4.1.0",
        "luxon": "^3.6.1",
        "moment-timezone": "^0.5.48",
        "winston": "^3.17.0",
        "zod": "^3.24.4"
    },
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/plugin-transform-modules-commonjs": "^7.27.1",
        "@babel/plugin-transform-typescript": "^7.27.1",
        "@babel/preset-typescript": "^7.27.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.26.0",
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@swc/core": "^1.11.24",
        "@theunwalked/cardigantime": "^0.0.1",
        "@doccident/doccident": "^0.0.1",
        "@types/jest": "^29.5.14",
        "@types/js-yaml": "^4.0.9",
        "@types/luxon": "^3.6.2",
        "@types/node": "^22.15.18",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "eslint": "^9.26.0",
        "eslint-plugin-import": "^2.31.0",
        "globals": "^16.1.0",
        "jest": "^29.7.0",
        "mockdate": "^3.0.5",
        "rollup-plugin-preserve-shebang": "^1.0.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "ts-jest": "^29.3.3",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.3",
        "vite-plugin-node": "^5.0.1"
    },
    "pnpm": {
        "overrides": {
            "wrap-ansi": "7.0.0"
        }
    }
}